
CREATE TABLE "clean_environment_data" (
    "Product" VARCHAR   NOT NULL,
    "NutrUnits" VARCHAR   NOT NULL,
    "LandUse" decimal   NOT NULL,
    "CO2Emissions" decimal   NOT NULL,
    "SO2Emissions" decimal   NOT NULL,
    "PO4Emissions" decimal   NOT NULL,
    "FreshwaterWithdrawals" decimal   NOT NULL,
	"Type" VARCHAR   NOT NULL,
	CONSTRAINT "pk_clean_environment_data" PRIMARY KEY (
        "Product"
	)
);